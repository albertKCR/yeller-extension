
{
    "manifest_version": 2,
    "name": "Yeller",
    "description": "This extension prevents you from opening twitter.",
    "version": "1.0",
    "background": {
      "scripts": ["background.js"]
    },
    "permissions": [
      "tabs"
    ],
    "browser_action": {
      "default_icon": "icon.png"
    }
  }